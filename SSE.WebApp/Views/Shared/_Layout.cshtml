<!DOCTYPE html>
<html lang="en-US" dir="ltr">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <!-- ===============================================-->
    <!--    Document Title-->
    <!-- ===============================================-->
    <title>@ViewData["title"] | VectoRank.com</title>


    <!-- ===============================================-->
    <!--    Favicons-->
    <!-- ===============================================-->
    <link rel="apple-touch-icon" sizes="180x180" href="~/assets/img/favicons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets/img/icons/logo.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets/img/icons/logo.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/img/icons/logo.png">
    <link rel="manifest" href="~/assets/img/favicons/manifest.json">
    <meta name="msapplication-TileImage" content="~/assets/img/favicons/mstile-150x150.png">
    <meta name="theme-color" content="#ffffff">


    <!-- ===============================================-->
    <!--    Stylesheets-->
    <!-- ===============================================-->
    <link href="~/assets/css/theme.css" rel="stylesheet" />

</head>


<body>

    <!-- ===============================================-->
    <!--    Main Content-->
    <!-- ===============================================-->
    <main class="main" id="top">
        <nav class="navbar navbar-expand-lg navbar-light fixed-top py-3" data-navbar-on-scroll="data-navbar-on-scroll">
            <div class="container">
                <a class="navbar-brand" href="~/"><img src="assets/img/icons/logo.png" alt="" width="30" /><span class="text-1000 fs-1 ms-2 fw-medium">Vecto<span class="fw-bold">R</span>ank</span></a>
                <button class="navbar-toggler collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav mx-auto border-bottom border-lg-bottom-0 pt-2 pt-lg-0">
                        <li class="nav-item"><a class="nav-link active active" aria-current="page" href="~/">Anasayfa</a></li>
                        @*                         <li class="nav-item"><a class="nav-link" href="#">Blog</a></li> *@
                        <li class="nav-item"><a class="nav-link" href="~/pricing">Fiyatlandırma</a></li>
                        <li class="nav-item"><a class="nav-link" href="~/partnership">PartnerShip(Footer)</a></li>
                        <li class="nav-item"><a class="nav-link" href="~/about">Hakkımızda</a></li>
                        <li class="nav-item"><a class="nav-link" href="~/contact">İletişim</a></li>
                    </ul>
                    <form class="d-flex py-3 py-lg-0">
                        <a class="btn btn-link text-1000 fw-medium order-1 order-lg-0" href="https://panel.vectorank.com/login">Giriş Yap</a>
                        <a class="btn btn-outline-danger rounded-pill order-0" href="https://panel.vectorank.com/signup">Ücretsiz Dene</a>
                    </form>
                </div>
            </div>
        </nav>

        @RenderBody()

        </section>
        <!-- <section> close ============================-->
        <!-- ============================================-->
        <!-- ============================================-->
        <!-- <section> begin ============================-->
        <section class=" bg-100 pt-4 pb-4">

            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="text-400 text-center">
                            <p>Copyright &copy; 2024 vectorank.com</p>
                        </div>
                    </div>
                </div>
            </div>
            <!-- end of .container-->

        </section>
        <!-- <section> close ============================-->
        <!-- ============================================-->


    </main>
    <!-- ===============================================-->
    <!--    End of Main Content-->
    <!-- ===============================================-->
    <!-- ===============================================-->
    <!--    JavaScripts-->
    <!-- ===============================================-->
    <script src="~/vendors/@@popperjs/popper.min.js"></script>
    <script src="~/vendors/bootstrap/bootstrap.min.js"></script>
    <script src="~/vendors/is/is.min.js"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=window.scroll"></script>
    <script src="~/vendors/jquery/jquery-3.7.1.min.js"></script>
    <script src="~/assets/js/theme.js"></script>
    <link href="https://fonts.googleapis.com/css2?family=Rubik:ital,wght@@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&amp;display=swap" rel="stylesheet">
</body>

</html>